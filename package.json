{
  "name": "my-vue-app",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "husky install & node ./scripts/update.js",
    "dev": "vite",
    "build": "tsc && vite build --base=https://bengalurucares.github.io/bengalurucares.in",
    "build:dev": "tsc && vite build",
    "postbuild": "./bin/bglcares post-build -c -d \"dist\" \"data/**/*.json\"",
    "postbuild:dev": "yarn postbuild",
    "serve": "vite preview",
    "lint": "eslint src --ext \"ts,tsx,js,jsx\"",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "immer": "^9.0.2",
    "modern-normalize": "^1.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "use-immer": "^0.5.1",
    "zustand": "^3.4.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@types/node-fetch": "^2.5.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "node-fetch": "^2.6.1",
    "postcss-env-function": "^3.0.0",
    "postcss-nested": "^5.0.5",
    "prettier": "^2.2.1",
    "progress": "^2.0.3",
    "typescript": "^4.1.2",
    "vite": "^2.2.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "yarn lint:fix"
    ]
  }
}
